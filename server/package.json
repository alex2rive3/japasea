{
  "name": "japasea-server",
  "version": "1.0.0",
  "description": "Backend server for Japasea application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --testEnvironment=node",
    "test:watch": "jest --watch --testEnvironment=node",
    "test:coverage": "jest --coverage --testEnvironment=node",
    "db:seed": "node scripts/seedDatabase.js seed",
    "db:clear": "node scripts/seedDatabase.js clear",
    "migrate:places": "node scripts/migratePlacesToMongoDB.js"
  },
  "keywords": [
    "express",
    "nodejs",
    "mvc",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.3"
  }
}
